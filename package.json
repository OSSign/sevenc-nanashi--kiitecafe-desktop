{
  "name": "kiitecafe-desktop",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "enable-pre-post-scripts": true,
  "scripts": {
    "predev": "rimraf dist/* dist-electron/*",
    "dev": "run-p dev:* serve:electron",
    "dev:vite": "vite",
    "dev:electron": "tsc -w -p ./tsconfig.electron.json",
    "serve:electron": "wait-on ./dist-electron/main.js && nodemon --watch ./dist-electron --exec electron .",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "electron": "^21.2.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9",
    "wait-on": "^6.0.1"
  }
}
